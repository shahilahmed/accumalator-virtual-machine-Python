fun cal $op $num1 $num2 start {	$result = 0 end	if $op == 0 then {		$result = $num1 + $num2 end		goto case1 end	}	@case1	if $op == 1 then {		$result = $num1 - $num2 end		goto case2 end	}	@case2	if $op == 2 then {		$result = $num1 * $num2 end		goto case3 end	}	@case3	if $op == 3 then {		$result = $num1 * $num2 end		goto exit end	}	exit	return $result}fun main start {	$op   = 0	$num1 = 4 end	$num2 = 2 end	print ( cal ( $op , $num1 , $num2 ) ) end	print_char ( 10 ) end	print_char ( 10 ) end}